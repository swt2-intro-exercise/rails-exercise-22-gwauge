<div id="<%= dom_id paper %>">
  <p>
    <strong>Title:</strong>
    <%= paper.title %>
  </p>

  <p>
    <strong>Venue:</strong>
    <%= paper.venue %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= paper.year %>
  </p>


<h2>Authors</h2>
<ul>
  <% @paper.authors do |author| %>
    <li>
      <%= link_to author.name, author_path(author) %>
    </li>
  <% end %>
</ul>

</div>
